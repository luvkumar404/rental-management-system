<%if(success && success.length){%>
<div
  class="alert alert-success mt-3 alert-dismissible fade show col-6 offset-3"
  role="alert"
>
  <button
    type="button"
    class="btn-close"
    data-bs-dismiss="alert"
    aria-label="Close"
  ></button>
  <%= success %>
</div>

<%}%>


<%if(error && error.length){%>
<div
  class="alert alert-danger mt-3 alert-dismissible fade show col-6 offset-3"
  role="alert"
>
  <button
    type="button"
    class="btn-close"
    data-bs-dismiss="alert"
    aria-label="Close"
  ></button>
  <%= error %>
</div>

<%}%>